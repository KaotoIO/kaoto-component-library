{
  "name": "kaoto-component-library",
  "version": "0.0.0",
  "description": "A wrapper around the Kaoto UI that exposes it as a consumable component library.",
  "license": "Apache-2.0",
  "main": "./dist/editor/index.js",
  "types": "./dist/editor/index.d.ts",
  "homepage": "https://www.kaoto.io/",
  "files": [
    "dist/editor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KaotoIO/kaoto-component-library.git"
  },
  "dependencies": {
    "monaco-editor-webpack-plugin": "^7.0.1",
    "react-error-boundary": "3.1.4",
    "react-flow-renderer": "10.3.6",
    "react-router-last-location": "2.0.1",
    "react-shadow": "19.0.3",
    "uniforms": "^3.6.2",
    "uniforms-bridge-json-schema": "^3.9.1",
    "uniforms-patternfly": "^4.7.5",
    "use-debounce": "8.0.1",
    "web-vitals": "2.1.4",
    "@kie-tools-core/editor": "0.0.0",
    "@kie-tools-core/monaco-editor": "0.0.0",
    "@kie-tools-core/guided-tour": "0.0.0",
    "@kie-tools-core/notifications": "0.0.0",
    "@kie-tools-core/workspace": "0.0.0",
    "monaco-editor": "^0.23.0",
    "@kie-tools-core/patternfly-base": "0.0.0",
    "combine-reducer": "^1.0.2",
    "immer": "^9.0.6",
    "lodash": "^4.17.21",
    "react-cool-onclickoutside": "^1.6.1",
    "react-monaco-editor": "^0.43.0",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1",
    "react-sortable-hoc": "^2.0.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.1.0",
    "uuid": "^8.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@patternfly/react-core": "^4.157.3",
    "@patternfly/react-icons": "^4.11.17",
    "@patternfly/react-charts": "^6.15.23",
    "csstype": "^3.0.11"
  },
  "scripts": {
    "lint": "pnpm run-script-if --bool \"$(build-env global.build.lint)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "test": "pnpm run-script-if --bool \"$(build-env global.build.test)\" --then \"jest --silent --verbose --passWithNoTests\"",
    "copy:css": "copyfiles -u 1 \"src/**/*.{sass,scss,css}\" dist/",
    "copy:images": "cpr ./static/images ./dist/editor/images",
    "build:dev": "rimraf dist && pnpm copy:css && pnpm copy:images && tsc -p tsconfig.dev.json",
    "build:prod": "pnpm lint && rimraf dist && pnpm copy:css && pnpm copy:images && tsc -p tsconfig.prod.json && pnpm test",
    "test:watch": "jest --watch",
    "start": "cross-env GENERATE_SOURCEMAP=false webpack serve -c ./dev-webapp/webpack.config.js --host 0.0.0.0 --env dev",
    "deploy": "gh-pages -d dist",
    "cy:open": "pnpm cypress open --project it-tests",
    "cy:run": "pnpm cypress run -b chrome --project it-tests",
    "test:it": "pnpm run-script-if --bool \"$(build-env global.build.testIT)\" --then \"pnpm rimraf ./dist-it-tests\" \"pnpm start-server-and-test start http-get://0.0.0.0:$(build-env pmmlEditor.dev.port) cy:run\""
  },
  "devDependencies": {
    "@kie-tools/build-env": "0.0.0",
    "@kie-tools-core/run-script-if": "0.0.0",
    "@kie-tools-core/webpack-base": "0.0.0",
    "gh-pages": "^3.2.3",
    "@kie-tools-core/keyboard-shortcuts": "0.0.0",
    "@kie-tools-core/operating-system": "0.0.0",
    "@kie-tools-core/envelope-bus": "0.0.0",
    "@kie-tools-core/envelope": "0.0.0",
    "@kie-tools-core/i18n": "0.0.0",
    "cross-env": "^5.2.1",
    "@types/lodash": "^4.14.168",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@types/uuid": "^8.3.0",
    "@4tw/cypress-drag-drop": "^2.2.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.8",
    "@storybook/react": "^6.5.9",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.1",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.44",
    "@types/react-dom": "18.0.3",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cypress": "9.7.0",
    "dotenv-webpack": "7.1.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "5.5.1",
    "file-loader": "^6.2.0",
    "file-selector": "^0.6.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "lint-staged": "12.4.3",
    "mini-css-extract-plugin": "2.6.1",
    "null-loader": "^4.0.1",
    "prettier": "2.7.1",
    "prop-types": "15.8.1",
    "rimraf": "3.0.2",
    "source-map-loader": "4.0.0",
    "style-loader": "3.3.1",
    "ts-jest": "27.1.4",
    "ts-loader": "9.3.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "tslib": "^2.3.1",
    "typescript": "4.4.4",
    "url-loader": "4.1.1",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.2",
    "webpack-merge": "^5.8.0",
    "whatwg-fetch": "^3.6.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/react"
    ]
  }
}
